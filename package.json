{
  "name": "blogger-numeric-pager",
  "version": "1.0.0",
  "description": "Blogger Numeric Pager is an advanced pagination for Blogger that works even in searches.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel watch:js serve",
    "compile": "npm-run-all compile:js minify:js",
    "watch:js": "rollup -c -w",
    "compile:js": "rollup -c",
    "minify:js": "terser dist/main.js --compress --mangle -o dist/main.min.js",
    "serve": "servor dist --reload --browse"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkreations/blogger-numeric-pager.git"
  },
  "keywords": [],
  "author": "zkreations <daniel@zkreations.com> (https://zkreations.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/zkreations/blogger-numeric-pager/issues"
  },
  "homepage": "https://github.com/zkreations/blogger-numeric-pager#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "browserslist": "^4.24.4",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.34.9",
    "servor": "^4.0.2",
    "terser": "^5.39.0"
  },
  "browserslist": [
    ">= 1%",
    "last 2 major versions",
    "not dead",
    "not edge <= 18",
    "not ie <= 11"
  ]
}
